    "#Using Chi Square Test for change in rent\n",
    "CA_obs = pd.Series([1717.75,1701.50,1759.75,1792.0,1951.50,2098.00], index=[\"2011\", \"2012\", \"2013\", \"2014\", \"2015\", \"2016\"])\n",
