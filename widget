    "\n",
    "CA_Chi_SQ_df = pd.DataFrame(CA_obs)\n",
    "\n",
    "CA_Chi_SQ_df[1] = [1215.5,1206.0,1222.5,1233.0,1290.5,1306.5]\n",
    "\n",
